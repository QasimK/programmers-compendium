# .editorconfig project file
# https://EditorConfig.org
root = true

[*]
charset = utf-8
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true

[*.md]
indent_style = space
indent_size = 4

[*.sh]
indent_style = space
indent_size = 4

[*.html]
indent_style = space
indent_size = 4
